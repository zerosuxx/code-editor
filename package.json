    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@types/ace": "0.0.42",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.2",
    "cordova-common": "^4.0.2",
